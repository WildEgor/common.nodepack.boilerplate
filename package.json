{
  "name": "@wildegor/common.nodepack.boilerplate",
  "version": "1.0.2",
  "description": "Package with common utils",
  "author": "",
  "private": false,
  "license": "UNLICENSED",
  "main": "modules/index.js",
  "types": "modules/index.d.ts",
  "repository": "git://github.com/wildegor/common.nodepack.boilerplate.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/wildegor",
    "repository": "https://github.com/wildegor/common.nodepack.boilerplate.git"
  },
  "files": [
    "modules"
  ],
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf modules",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch --path tsconfig.json",
    "start:debug": "nest start --debug --watch",
    "dev:lint": "eslint src/**/*.ts",
    "dev:token": "node scripts/npm-token.js",
    "ci:token": "node scripts/ci-token.js",
    "dev:fix": "yarn dev:lint -- --fix",
    "dev:check": "tsc --noEmit --project tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "semantic-release",
    "prepare": "husky install && yarn build"
  },
  "devDependencies": {
    "@fastify/static": "6.6.0",
    "@nestjs/cli": "9.0.0",
    "@nestjs/common": "9.0.2",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.2",
    "@nestjs/cqrs": "9.0.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/mercurius": "^10.1.7",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-fastify": "9.0.2",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/swagger": "6.1.4",
    "@nestjs/terminus": "9.1.4",
    "@nestjs/testing": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@types/amqplib": "^0.10.1",
    "@types/jest": "28.1.4",
    "@types/microseconds": "0.2.0",
    "@types/node": "18.7.1",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "amqp-connection-manager": "4.1.10",
    "amqplib": "0.10.3",
    "axios": "0.27.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "fastify": "4.2.0",
    "fastify-plugin": "4.1.0",
    "graphql": "16.6.0",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "mercurius": "^11.5.0",
    "microseconds": "0.2.0",
    "mongoose": "6.8.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "semantic-release": "19.0.5",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "type-fest": "2.16.0",
    "typescript": "4.7.4",
    "uuid": "9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
